{
    "builds": [
        {
            "src": "job_board_backend/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "job_board_backend/wsgi.py"
        }
    ]
}
